@model IEnumerable<Resource>

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">List of position to borrow:</h2>
            <hr />
        </div>
        <table class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>
                        Title
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td width="20%">
                            @obj.Title
                        </td>
                    </tr>
                }
            </tbody>

        </table>
        <button type="submit" class="btn btn-primary" style="width:150px">Accept</button>
        <a asp-controller="Resources" asp-action="Resources" class="btn btn-secondary" style="width:150px">
            Back to list
        </a>
    </div>
</form>